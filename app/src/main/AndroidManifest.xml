<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.freshfarm">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".user.UserActivity"></activity>
        <activity android:name=".choose.ChooseItemActivity" />
        <activity android:name=".choose.ChooseMenuItemActivity" />
        <activity android:name=".choose.ChooseActivity" />
        <activity android:name=".user.signup.SignUpActivity" />
        <activity android:name=".user.login.LoginActivity" />
        <activity android:name=".MapTutorialActivity" />

        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="teyhoqytpg" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>